@{
    ViewBag.Title = "Suppression facture";
}


<div class="flex-content flex-jcenter-acenter flex-col pop-up">
    <div class="popup-container with-shadow">
        <div class="head gradient-blue">
            <h1>Attention !</h1>
        </div>
        <div class="content-section text-centered icon-bottom">
            <i class="pop-icon green-bg"></i>
            <p>Voulez-vous vraiment supprimer la sélection ?</p>
            <form action="" method="post">
                <input type="submit" value="Non" class="btn btn-primary" data-dismiss="modal" onclick="DecheckedFactureClick()">
                <input type="submit" value="Supprimer" class="btn btn-warning" onclick="@("window.location.href='" + @Url.Action("DeleteAllFactureSelected", "Facture") + "' ");">
            </form>
        </div>
    </div>

</div>

